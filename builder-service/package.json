{
  "name": "@fhiriq/builder-service",
  "version": "1.0.0",
  "description": "FHIR IQ Builder Service - External service for generating FHIR application scaffolds",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
